{
  "name": "makotools-link-preview",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@fontsource/sora": "^4.5.10",
    "@vercel/og": "^0.0.20",
    "chrome-aws-lambda": "8.0.2",
    "inter-ui": "^3.19.3",
    "next": "^13.0.3",
    "playwright-aws-lambda": "^0.7.0",
    "playwright-core": "^1.16.3",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "0.0.44",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@types/react": "^18.0.25",
    "typescript": "^4.9.3"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "prettier"
    ],
    "plugins": [
      "unused-imports",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "warn"
      ],
      "no-nested-ternary": [
        "off"
      ],
      "react/self-closing-comp": [
        "warn"
      ],
      "react/jsx-props-no-spreading": [
        "off"
      ],
      "import/order": [
        "warn",
        {
          "newlines-between": "always"
        }
      ],
      "import/first": [
        "warn"
      ],
      "@typescript-eslint/no-unused-vars": "off",
      "unused-imports/no-unused-imports": "warn"
    }
  },
  "_comment": "Add empty prettier key in order to override code editor settings while applying all Prettier defaults",
  "prettier": {},
  "packageManager": "yarn@3.2.2",
  "resolutions": {
    "satori": "0.0.44"
  }
}
